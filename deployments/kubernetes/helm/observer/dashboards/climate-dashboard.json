          },
          "query": "from(bucket: \"homematic\")\n  |> range(start: v.timeRangeStart, stop:v.timeRangeStop)\n  |> filter(fn: (r) =>\n    r._measurement == \"device\" and\n    r._field == \"actual_temperature\" and\n    r.device_type == \"TEMPERATURE_HUMIDITY_SENSOR_OUTDOOR\"\n  )\n  |> group(columns: [\"device_name\"])\n  |> keep(columns: [\"_time\", \"_value\", \"device_name\"])",
          "refId": "A"
        }
      ],
      "title": "Temperatur",
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": "device\\.temp \\{device_name: ([A-Za-z]+)\\}",
            "renamePattern": "$1"
          }
        }
      ],
